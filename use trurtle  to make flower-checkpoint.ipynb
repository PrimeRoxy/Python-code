    "from turtle import *\n",
    "\n",
    "def fleur():\n",
    "    for i in range(300):\n",
    "        circle(190-i,90)\n",
    "        left(90)\n",
    "        circle(190-i,90)\n",
    "        left(18)\n",
    "        \n",
    "        \n",
    "fleur()\n",
    "mainloop()"
   ]
